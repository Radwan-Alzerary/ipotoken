<div>
  <h1 class="text-3xl font-bold mb-6">إدارة الأجهزة</h1>
  
  <div class="bg-white p-6 rounded shadow">
    <h2 class="text-2xl font-semibold mb-4">الأجهزة</h2>
    <div class="overflow-x-auto">
      <table class="min-w-full divide-y divide-gray-200">
        <thead class="bg-gray-100">
          <tr>
            <th class="px-4 py-2 text-right text-sm font-medium text-gray-600">مفتاح الجهاز</th>
            <th class="px-4 py-2 text-right text-sm font-medium text-gray-600">IP</th>
            <th class="px-4 py-2 text-right text-sm font-medium text-gray-600">الرمز</th>
            <th class="px-4 py-2 text-right text-sm font-medium text-gray-600">التاريخ</th>
          </tr>
        </thead>
        <tbody class="divide-y divide-gray-200">
          <% users.forEach(user => { %>
            <tr>
              <td class="px-4 py-2"><%= user.deviceKey %></td>
              <td class="px-4 py-2"><%= user.ip %></td>
              <td class="px-4 py-2"><%= user.token %></td>
              <td class="px-4 py-2"><%= new Date(user.timestamp).toLocaleString() %></td>
            </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
